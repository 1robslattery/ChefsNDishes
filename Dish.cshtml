@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h2 class="display-4">Add a Dish!!</h2>
</div>

<p><a href="/"><b>Home</b></a>

<a href="/AddDish">"Add a Dish"</a>

<p>Yum, take a look at our tasty dishes!</p>

<hr>
@model Dish
    <div class="container">
        <div class="table-responsive">
            <table class="table table-striped table-bordered" style="width: 800px;">
                <tr>
                    <th>Name</th>
                    <th>Chef</th>
                    <th>Tastiness</th>
                    <th>Calories</th>
                </tr>
            @{
                foreach(var d in ViewBag.All)
                {
                <tr>
                    <td>@d.DishName</td>
                    <td>@d.Cook.FirstName</td>
                    <!-- <td>cook</td> -->
                    <td>@d.Tastiness</td>
                    <td>@d.Calories</td>
                </tr>
                }
            }
            </table>
        </div>
    </div>